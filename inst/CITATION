citHeader("To cite package SLmisc in publications use:")

citEntry(entry="Manual",
         title = "SLmisc: Miscellaneous functions for analysis of gene expression data at SIRS-Lab GmbH",
         author = personList(as.person("M. Kohl")),
         language = "English",
         year = 2007,
         organization = "SIRS-Lab GmbH",
         type = "R package",
         address = "Jena",
         url = "www.sirs-lab.com",
textVersion = paste("Kohl, M., (2007).", "SLmisc: Miscellaneous functions for analysis of gene expression data at SIRS-Lab GmbH.", "R package,", "SIRS-Lab GmbH.", "Jena."))
